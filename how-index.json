        "template--16839176192214__5cc501d5-fd07-458d-99ee-5f75d92679aa-167707081731afcde0-2": {
          "type": "text",
          "settings": {
            "title": "When will my order ship?",
            "subtitle": "",
            "icon": "no-icon",
            "button_label": "",
            "button_link": "",
            "body": "<p>All orders will be shipped within 1 to 2 business days. Once the shipping label is created, you will receive a shipping confirmation via email with tracking details. Please note fulfillment and shipping do not take place outside of business hours or on weekends or holidays. <\/p>"
          }
