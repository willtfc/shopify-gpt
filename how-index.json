        "6bb9389b-b3a5-4109-9f69-4ac9e68d7683": {
          "type": "text",
          "settings": {
            "title": "Can I travel abroad with my CBD products?",
            "subtitle": "",
            "icon": "no-icon",
            "button_label": "",
            "button_link": "",
            "body": "<p>The U.S. allows the travel of hemp-derived CBD domestically. Internationally, however, laws differ by country, so be sure to check the policies of your destination. For example, most EU countries allow products with 0.2% THC or less. <\/p>"
          }
